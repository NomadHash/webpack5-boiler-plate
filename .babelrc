{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "modules": false,
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@emotion",
        [
            "module-resolver",
            {
                "alias": {
                    "@src": "./src/",
                    "@components": "./src/components/",
                    "@lib": "./src/lib/"
                }
            }
        ]
    ]
}